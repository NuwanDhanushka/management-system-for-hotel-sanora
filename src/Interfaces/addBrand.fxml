                  </Pane>
                  <GridPane layoutX="87.0" layoutY="121.0" prefHeight="345.0" prefWidth="313.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="230.0" minWidth="10.0" prefWidth="230.0" />
