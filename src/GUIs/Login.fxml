<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="350.0" prefWidth="580.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginController">
   <children>
      <ImageView fitHeight="350.0" fitWidth="580.0" pickOnBounds="true">
         <image>
            <Image url="@../Images/login.png" />
         </image>
      </ImageView>
      <PasswordField fx:id="passwordField" focusTraversable="false" layoutX="182.0" layoutY="212.0" promptText="             Password">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </PasswordField>
      <TextField fx:id="usernameField" focusTraversable="false" layoutX="182.0" layoutY="163.0" promptText="            User Name">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
      <Button layoutX="240.0" layoutY="263.0" mnemonicParsing="false" onAction="#loginAddButtonAction" prefHeight="25.0" prefWidth="89.0" stylesheets="@../Styles/popUp.css" text="LOGIN" textFill="TRANSPARENT" />
   </children>
</AnchorPane>
